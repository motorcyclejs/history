{
  "name": "@motorcycle/history",
  "version": "2.1.2",
  "description": "Standard History driver for Motorcycle.js",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "test:lint": "tslint src/**/*.ts src/*.ts",
    "test:unit": "TS_NODE_PROJECT=./test/tsconfig.json mocha -r ts-node/register test/node/*.ts",
    "test:browser:pre": "rimraf test/.tmp test/browser/bundle.js && tsc -P test/tsconfig.json",
    "test:browser:hash": "npm run test:browser:pre && browserify test/.tmp/test/browser/createHashHistory.js -o test/browser/bundle.js && karma start --single-run",
    "test:browser:history": "npm run test:browser:pre && browserify test/.tmp/test/browser/createBrowserHistory.js -o test/browser/bundle.js && karma start --single-run",
    "test:browser": "npm run test:browser:hash && npm run test:browser:history",
    "test": "npm run test:lint && npm run test:unit && npm run test:browser",
    "commit": "git-cz",
    "changelog": "conventional-changelog --infile CHANGELOG.md --same-file --release-count 0 --preset angular",
    "build": "rimraf lib && tsc",
    "preversion": "npm run build",
    "postversion": "git push origin master && git push origin --tags && npm publish && npm run changelog",
    "release:minor": "npm version minor -m 'chore(package): v%s'",
    "release:major": "npm version major -m 'chore(package): v%s'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/motorcyclejs/history.git"
  },
  "keywords": [
    "typescript"
  ],
  "author": {
    "name": "Tylor Steinberger",
    "email": "tlsteinberger167@gmail.com",
    "url": "https://github.com/Tylors"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/motorcyclejs/history/issues"
  },
  "homepage": "https://github.com/motorcyclejs/history#readme",
  "dependencies": {
    "@motorcycle/core": "^1.6.0",
    "history": "^4.4.0",
    "most": "^1.1.0",
    "most-subject": "^5.2.0"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.33",
    "@types/node": "^6.0.48",
    "browserify": "^13.1.1",
    "commitizen": "^2.8.6",
    "conventional-changelog-cli": "^1.2.0",
    "cz-conventional-changelog": "^1.2.0",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-mocha": "^1.3.0",
    "mocha": "^3.1.2",
    "rimraf": "^2.5.4",
    "ts-node": "^1.7.0",
    "tslint": "^4.0.0-dev.1",
    "typescript": "^2.2.0-dev.20161116"
  },
  "peerDependencies": {}
}
